#include <bits/stdc++.h> 
#include <bits/stdc++.h> 
int findDuplicate(vector<int> &arr, int n){
 
    sort(arr.begin(),arr.end());
    for(int i=0;i<n;i++)
    {
        if(i+1<n)
        {
            if(arr[i]==arr[i+1])
            {
               return arr[i];
               break;  
            }
               
        }
    }
}
